                    %i.fas.fa-arrow-down.text-warning.mr-3
                    46,53%
                %tr
                  %th{:scope => "row"}
                    \/argon/charts.html
                  %td
                    3,513
                  %td
                    294
                  %td
                    %i.fas.fa-arrow-down.text-warning.mr-3
                    36,49%
                %tr
                  %th{:scope => "row"}
                    \/argon/tables.html
                  %td
                    2,050
                  %td
                    147
                  %td
                    %i.fas.fa-arrow-up.text-success.mr-3
                    50,87%
                %tr
                  %th{:scope => "row"}
                    \/argon/profile.html
                  %td
                    1,795
                  %td
                    190
                  %td
                    %i.fas.fa-arrow-down.text-danger.mr-3
                    46,53%
      .col-xl-4
        .card.shadow
          .card-header.border-0
            .row.align-items-center
              .col
                %h3.mb-0 Social traffic
              .col.text-right
                %a.btn.btn-sm.btn-primary{href: "#!"} See all
          .table-responsive
            / Projects table
            %table.table.align-items-center.table-flush
              %thead.thead-light
                %tr
                  %th{:scope => "col"} Referral
                  %th{:scope => "col"} Visitors
                  %th{:scope => "col"}
              %tbody
                %tr
                  %th{:scope => "row"}
                    Facebook
                  %td
                    1,480
                  %td
                    .d-flex.align-items-center
                      %span.mr-2 60%
                      %div
                        .progress
                          .progress-bar.bg-gradient-danger{"aria-valuemax": "100", "aria-valuemin": "0", "aria-valuenow": "60", :role => "progressbar", :style => "width: 60%;"}
                %tr
                  %th{:scope => "row"}
                    Facebook
                  %td
                    5,480
                  %td
                    .d-flex.align-items-center
                      %span.mr-2 70%
                      %div
                        .progress
                          .progress-bar.bg-gradient-success{"aria-valuemax": "100", "aria-valuemin": "0", "aria-valuenow": "70", :role => "progressbar", :style => "width: 70%;"}
                %tr
