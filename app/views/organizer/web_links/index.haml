/ Side navigation
= render "organizer/shared/navigation"

.main-content
  / Top navbar
  %nav#navbar-main.navbar.navbar-top.navbar-expand-md.navbar-dark
    .container-fluid
      / Logo
      %a.h4.mb-0.text-white.text-uppercase.d-none.d-lg-inline-block{href: organizer_dashboard_path} Dashboard

      / User profile dropdown
      %ul.navbar-nav.align-items-center.d-none.d-md-flex
        %li.nav-item.dropdown
          %a.nav-link.pr-0{"aria-expanded": "false", "aria-haspopup": "true", "data-toggle": "dropdown", href: "#", :role => "button"}
            .media.align-items-center
              %span.avatar.avatar-sm.rounded-circle
                = image_tag("sketch.jpg")
              .media-body.ml-2.d-none.d-lg-block
                %span.mb-0.text-sm.font-weight-bold== #{current_user.first_name} #{current_user.last_name}
          = render "organizer/shared/profile_navigation"

  / Header
  .header.bg-gradient-primary.pb-8.pt-5.pt-md-8
    .container-fluid
      .header-body



        / Card stats
        .row
          .col-xl-3.col-lg-6
            .card.card-stats.mb-4.mb-xl-0
              .card-body
                .row
                  .col
                    %h5.card-title.text-uppercase.text-muted.mb-0 Total Surveys
                    %span.h2.font-weight-bold.mb-0 14
                  .col-auto
                    .icon.icon-shape.bg-danger.text-white.rounded-circle.shadow
                      %i.fas.fa-chart-bar
                %p.mt-3.mb-0.text-muted.text-sm
                  %span.text-success.mr-2
                    %i.fa.fa-arrow-up
                    3.48%
                  %span.text-nowrap Since last month

          .col-xl-3.col-lg-6
            .card.card-stats.mb-4.mb-xl-0
              .card-body
                .row
                  .col
                    %h5.card-title.text-uppercase.text-muted.mb-0 Top Charisms
                    %p.h4.font-weight-bold.mb-0 Administration
                    %p.h4.font-weight-bold.mb-0 Mercy
                    %p.h4.font-weight-bold.mb-0 Hospitality
                  .col-auto
                    .icon.icon-shape.bg-warning.text-white.rounded-circle.shadow
                      %i.fas.fa-chart-pie

          .col-xl-3.col-lg-6
            .card.card-stats.mb-4.mb-xl-0
              .card-body
                .row
                  .col
                    %h5.card-title.text-uppercase.text-muted.mb-0 Survey Completion
                    %span.h2.font-weight-bold.mb-0 49,65%
                  .col-auto
                    .icon.icon-shape.bg-info.text-white.rounded-circle.shadow
                      %i.fas.fa-percent
                %p.mt-3.mb-0.text-muted.text-sm
                  %span.text-success.mr-2
                    %i.fas.fa-arrow-up
                    12%
                  %span.text-nowrap Since last month

          .col-xl-3.col-lg-6
            .card.card-stats.mb-4.mb-xl-0
              .card-body
                .row
                  .col
                    %h5.card-title.text-uppercase.text-muted.mb-0 Top Opportunities
                    %p.h4.font-weight-bold.mb-0 Lector
                    %p.h4.font-weight-bold.mb-0 EMHC
                    %p.h4.font-weight-bold.mb-0 Server
                  .col-auto
                    .icon.icon-shape.bg-warning.text-white.rounded-circle.shadow
                      %i.fas.fa-chart-pie

  / Page content
  .container-fluid.mt--7
    .row
      .col-xl-8.mb-5.mb-xl-0
        .card.bg-gradient-default.shadow
          .card-header.bg-transparent
            .row.align-items-center
              .col
                %h6.text-uppercase.text-light.ls-1.mb-1 Summary
                %h2.text-white.mb-0 Top Charisms
          .card-body
            .chart
              / Chart wrapper
              %canvas#pie-chart-top-charisms.chart-canvas

    .row.mt-5
      .col-xl-8.mb-5.mb-xl-0
        .card.shadow
          .card-header.border-0
            .row.align-items-center
              .col
                %h3.mb-0 Page visits
              .col.text-right
                %a.btn.btn-sm.btn-primary{href: "#!"} See all
          .table-responsive
            / Projects table
            %table.table.align-items-center.table-flush
              %thead.thead-light
                %tr
                  %th{:scope => "col"} Page name
                  %th{:scope => "col"} Visitors
                  %th{:scope => "col"} Unique users
                  %th{:scope => "col"} Bounce rate
              %tbody
                %tr
                  %th{:scope => "row"}
                    \/argon/
                  %td
                    4,569
                  %td
                    340
                  %td
                    %i.fas.fa-arrow-up.text-success.mr-3
                    46,53%
                %tr
                  %th{:scope => "row"}
                    \/argon/index.html
                  %td
                    3,985
                  %td
                    319
